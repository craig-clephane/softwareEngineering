// ATM interface
interface ATM{
	void balance();
	void withdraw();
	void printReceipt();
}

// Example use of ATM interface
public class account implements ATM{
	public void balance(){
		System.out.println("Balance is Â£10");
	}  
	public void withdraw(){
		System.out.println("Withdraw Successful");
	}  
	public void printReceipt(){
		System.out.println("This is your receipt");
	}

	public static void main(String args[]){  
		ATM obj = new account();  
		obj.balance(); 
		obj.withdraw();
		obj.printReceipt();  
	} 
}

